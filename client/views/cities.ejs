<%- include("partials/header") %>
    Display cities
    <% cities.forEach((city) => { %>
        <div>
            <h2><%= city.city %></h2>
            <h3><%= city.country %></h3>
            <h3><%= city.description %></h3>
            <img src="<%= city.img %>">
        </div>
    <% }) %>
    
